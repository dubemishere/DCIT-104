num = input("Input the any given number")
num = int(num)
sum = 0
prime = 0
for i in range(2, num):
    for j in range(2, i):
        if (i % j == 0):
            break
    else:
        print(i)
        sum = sum + i
        prime = prime + 1

print("Sum =", sum)
print("average of prime is", sum / prime)
